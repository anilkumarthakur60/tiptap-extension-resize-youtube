{
  "name": "@anilkumarthakur/tiptap-extension-resize-youtube",
  "version": "0.0.2",
  "private": false,
  "type": "module",
  "main": "dist/index.umd.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ignore-pattern node_modules/ --ignore-pattern dist/ --ignore-pattern package-lock.json",
    "format": "prettier --write src/",
    "format:check": "prettier --check src/"
  },
  "devDependencies": {
    "@eslint/js": "^9.20.0",
    "@types/node": "^22.13.4",
    "eslint": "^9.20.1",
    "globals": "^16.0.0",
    "prettier": "3.5.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.2.0",
    "vite-plugin-dts": "^4.5.0"
  },
  "peerDependencies": {
    "@tiptap/extension-youtube": "^2.11.3"
  },
  "keywords": [
    "tiptap",
    "extension",
    "youtube",
    "resize",
    "video",
    "plugin",
    "media",
    "responsive",
    "embed"
  ]
}
