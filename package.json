{
  "name": "@anilkumarthakur/tiptap-extension-resize-youtube",
  "version": "0.0.3",
  "private": false,
  "type": "module",
  "main": "dist/index.umd.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ignore-pattern node_modules/ --ignore-pattern dist/ --ignore-pattern package-lock.json",
    "format": "prettier --write src/",
    "format:check": "prettier --check src/"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@types/node": "^24.0.3",
    "eslint": "^9.29.0",
    "globals": "^16.4.0",
    "prettier": "3.5.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.34.1",
    "vite": "^6.3.5",
    "vite-plugin-dts": "^4.5.4"
  },
  "peerDependencies": {
    "@tiptap/extension-youtube": "^2.11.3"
  },
  "keywords": [
    "tiptap",
    "extension",
    "youtube",
    "resize",
    "video",
    "plugin",
    "media",
    "responsive",
    "embed"
  ]
}
